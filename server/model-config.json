{
  "_meta": {
    "sources": [
      "loopback/common/models",
      "loopback/server/models",
      "../common/models",
      "./models",
      "../node_modules/loopback-component-passport/lib/models"
    ],
    "mixins": [
      "loopback/common/mixins",
      "loopback/server/mixins",
      "../common/mixins",
      "../node_modules/loopback-ds-computed-mixin/lib",
      "../node_modules/loopback-ds-calculated-mixin/lib",
      "../node_modules/loopback-ds-timestamp-mixin",
      "./mixins"
    ]
  },
  "Email": {
    "dataSource": "email",
    "public": false
  },
  "AccessToken": {
    "dataSource": "db",
    "public": false,
    "relations": {
      "user": {
        "type": "belongsTo",
        "model": "Person",
        "foreignKey": "userId"
      }
    }
  },
  "ACL": {
    "dataSource": "db",
    "public": false
  },
  "RoleMapping": {
    "dataSource": "db",
    "public": false,
    "options": {
      "strictObjectIDCoercion": true
    },
    "relations": {}
  },
  "Role": {
    "dataSource": "db",
    "public": false,
    "relations": {
      "permissions": {
        "name": "permissions",
        "type": "hasMany",
        "model": "ACL",
        "polymorphic": {
          "foreignKey": "principalId",
          "discriminator": "principalType"
        },
        "options": {
          "nestRemoting": true
        }
      },
      "users": {
        "type": "hasMany",
        "model": "Person",
        "polymorphic": "principal",
        "through": "RoleMapping",
        "invert": true,
        "options": {
          "nestRemoting": true
        }
      }
    }
  },
  "Person": {
    "dataSource": "db",
    "public": true
  }
}
